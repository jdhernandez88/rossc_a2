#!/bin/bash

source /opt/ros/indigo//setup.bash
source ~/catkin_ws/devel/setup.bash

rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 5.0, right: 5.0}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 0.3, right: 0.3}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 0.0, right: 0.0}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: -0.39, right: 0.39}'

rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 5.0, right: 5.0}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 0.3, right: 0.3}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 0.0, right: 0.0}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: -0.39, right: 0.39}'

rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 5.0, right: 5.0}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 0.3, right: 0.3}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 0.0, right: 0.0}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: -0.39, right: 0.39}'

rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 5.0, right: 5.0}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 0.3, right: 0.3}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: 0.0, right: 0.0}'
rostopic pub -1 \cmd_drive kingfisher_msgs/Drive  '{left: -0.39, right: 0.39}'
